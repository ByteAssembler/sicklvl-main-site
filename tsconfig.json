{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "$assets/*": [
                "/src/assets/*"
            ],
            "$components/*": [
                "/src/components/*"
            ],
            "$wrappers/*": [
                "/src/wrappers/*"
            ],
            "$components": [
                "/src/components"
            ],
            "$layouts/*": [
                "/src/layouts/*"
            ],
            "$pages/*": [
                "/src/pages/*"
            ]
        }
    }
}